{
  "name": "test-turtle",
  "version": "6.0.0",
  "keywords": [
    "test",
    "runner",
    "memoization"
  ],
  "scripts": {
    "lint": "npx eslint 'lib/*.mjs' 'bin/*.mjs' 'test/*.mjs'",
    "format": "npx prettier -w 'lib/*.mjs' 'bin/*.mjs' 'test/*.mjs'",
    "test": "node bin/test-turtle.mjs --plugin ./test/plugin.mjs"
  },
  "files": [
    "lib/*.mjs",
    "bin/*.mjs"
  ],
  "main": "lib/index.js",
  "bin": {
    "test-turtle": "lib/bin.mjs"
  },
  "devDependencies": {
    "c8": "^7.12.0",
    "chalk": "^5.0.1",
    "eslint": "^8.23.0",
    "prettier": "^2.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lachrist/test-turtle.git"
  },
  "dependencies": {
    "minimist": "^1.2.6"
  }
}
