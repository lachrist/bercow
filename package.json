{
  "name": "test-turtle",
  "version": "0.0.2",
  "keywords": [
    "test",
    "runner",
    "memoization",
    "coverage",
    "c8"
  ],
  "scripts": {
    "lint": "npx eslint 'lib/*.js'",
    "test": "node lib/assert.test.js && node lib/main.test.js && node lib/bin.test.js",
    "test-cov": "node lib/bin.js lib",
    "prettier": "npx prettier --write 'lib/*.js'"
  },
  "files": [
    "/lib/assert.js",
    "/lib/main.js",
    "/lib/bin.js"
  ],
  "main": "/lib/main.js",
  "bin": {
    "test-turtle": "/lib/bin.js",
    "tt": "/lib/bin.js"
  },
  "devDependencies": {
    "eslint": "^7.29.0",
    "prettier": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lachrist/test-turtle.git"
  },
  "dependencies": {
    "c8": "^7.7.3",
    "chalk": "^4.1.1",
    "minimist": "^1.2.5"
  }
}
