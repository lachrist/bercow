{
  "name": "test-turtle",
  "version": "1.0.0",
  "keywords": [
    "test",
    "runner",
    "memoization",
    "coverage",
    "c8"
  ],
  "scripts": {
    "test": "node lib/bin.js lib --each 'npx eslint $1 $2' --each 'npx prettier -w $1 $2' --each 'node $2' --each 'npx c8 --check-coverage --branches 100 --functions 100 --include $1 -- node $2'"
  },
  "files": [
    "/lib/assert.js",
    "/lib/main.js",
    "/lib/bin.js"
  ],
  "main": "/lib/main.js",
  "bin": {
    "test-turtle": "/lib/bin.js",
    "tt": "/lib/bin.js"
  },
  "devDependencies": {
    "c8": "^7.7.3",
    "eslint": "^7.29.0",
    "prettier": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lachrist/test-turtle.git"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "commitizen": "^4.2.4",
    "g": "^2.0.1",
    "minimist": "^1.2.5"
  }
}
