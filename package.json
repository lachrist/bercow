{
  "name": "test-turtle",
  "version": "2.0.0",
  "keywords": [
    "test",
    "runner",
    "memoization",
    "coverage",
    "c8"
  ],
  "scripts": {
    "lint": "npx eslint 'lib/*.mjs'",
    "prettify": "npx prettier -w 'lib/*.mjs'",
    "test": "node lib/util.test.mjs && node lib/either.test.mjs && node lib/index.test.mjs && node lib/main.test.mjs && node lib/bin.test.mjs",
    "turtle": "node bin/bin.mjs 'npx eslint $1 $2 && npx prettier -w $1 $2 && node $2 && npx c8 --check-coverage --branches 100 --functions 100 --include $1 -- node $2'"
  },
  "files": [
    "/lib/assert.mjs",
    "/lib/log.mjs",
    "/lib/spawn.mjs",
    "/lib/index.mjs",
    "/lib/main.mjs",
    "/bin/bin.mjs"
  ],
  "main": "/lib/index.js",
  "bin": {
    "test-turtle": "/bin/bin.mjs"
  },
  "devDependencies": {
    "c8": "^7.7.3",
    "eslint": "^7.29.0",
    "prettier": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lachrist/test-turtle.git"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "list-lang": "^0.0.0",
    "minimist": "^1.2.5"
  }
}
